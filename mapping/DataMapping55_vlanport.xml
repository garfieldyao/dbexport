<?xml version="1.0" encoding="UTF-8"?>
<mappings xmlns="http://www.garfield.com/MappingSchema"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.garfield.com/MappingSchema MappingSchema.xsd ">
	<!-- configure vlan id -->
	<mapping>
		<attributes>
			<prefix>configure bridge</prefix>
			<miniAttributes>2</miniAttributes>
			<sortor>com.mars.dbexport.service.sortor.GenericSortor</sortor>
			<attribute>
				<cliname>port</cliname>
				<dbTable>portVlanTbl</dbTable>
				<dbAttribute>logic</dbAttribute>
				<index>slot;logic</index>
				<parser>route(portTbl?(slot,2,4:bsSlot,2,4)(logPort,2,4:bsLog,2,4))(multi(xref|1/1/|lanConfig|(bsSlot,2,4:index,2,4)(bsLog,2,4:itfmLog,2,4)|(index,16,8,-2)(raw:/)(index,7,9,1)(raw:/)(index,0,7,1)(raw:/)(slot,0,0,0)(raw:/)(port,0,0,0);xref|1/1/|voipUni|(bsSlot,2,4:index,2,4)(bsLog,2,4:itfmLog,2,4)|(index,16,8,-2)(raw:/)(index,7,9,1)(raw:/)(index,0,7,1)(raw:/)(raw:vuni)))</parser>
			</attribute>
			<attribute>
				<cliname>vlan-id</cliname>
				<dbTable>portVlanTbl</dbTable>
				<dbAttribute>uvid</dbAttribute>
				<index>slot;logic</index>
				<parser>class:com.mars.dbexport.service.parse.VlanIdParser</parser>
			</attribute>
			<attribute>
				<cliname>network-vlan</cliname>
				<dbTable>portVlanTbl</dbTable>
				<dbAttribute>uvid</dbAttribute>
				<index>slot;logic</index>
				<parser>select(istrans)?((0x0?route(attachtbl?(slot,2,4:slot,2,4)(logic,2,4:logic,2,4))(fwdId>>class:com.mars.dbexport.service.parse.VlanIdParser))(0x1?raw:stacked:0:4097))</parser>
				<default>stacked:0:4097</default>
			</attribute>
			<attribute>
				<cliname>vlan-scope</cliname>
				<dbTable>portVlanTbl</dbTable>
				<dbAttribute>istrans</dbAttribute>
				<index>slot;logic</index>
				<parser>enum:(0x0,local)(0x1,network)</parser>
				<default>network</default>
			</attribute>
			<attribute>
				<cliname>qos</cliname>
				<dbTable>portVlanTbl</dbTable>
				<dbAttribute>ply</dbAttribute>
				<index>slot;logic</index>
				<parser>select(ply)?((0x1?prior>>>regx|priority:|(0,0,0))(0x2?regid>>>ref|profile:|(regtbl:id:name)))</parser>
			</attribute>
			<attribute>
				<cliname>qos-profile</cliname>
				<dbTable>portVlanTbl</dbTable>
				<dbAttribute>qosid</dbAttribute>
				<index>slot;logic</index>
				<parser>ref|name:|(QosSessions:Index:Name)</parser>
			</attribute>
			<attribute>
				<cliname>prior-best-effort</cliname>
				<dbTable>portVlanTbl</dbTable>
				<dbAttribute>plist</dbAttribute>
				<index>slot;logic</index>
				<parser>bool:7</parser>
				<novalue>true</novalue>
			</attribute>
			<attribute>
				<cliname>prior-background</cliname>
				<dbTable>portVlanTbl</dbTable>
				<dbAttribute>plist</dbAttribute>
				<index>slot;logic</index>
				<parser>bool:6</parser>
				<novalue>true</novalue>
			</attribute>
			<attribute>
				<cliname>prior-spare</cliname>
				<dbTable>portVlanTbl</dbTable>
				<dbAttribute>plist</dbAttribute>
				<index>slot;logic</index>
				<parser>bool:5</parser>
				<novalue>true</novalue>
			</attribute>
			<attribute>
				<cliname>prior-exc-effort</cliname>
				<dbTable>portVlanTbl</dbTable>
				<dbAttribute>plist</dbAttribute>
				<index>slot;logic</index>
				<parser>bool:4</parser>
				<novalue>true</novalue>
			</attribute>
			<attribute>
				<cliname>prior-ctrl-load</cliname>
				<dbTable>portVlanTbl</dbTable>
				<dbAttribute>plist</dbAttribute>
				<index>slot;logic</index>
				<parser>bool:3</parser>
				<novalue>true</novalue>
			</attribute>
			<attribute>
				<cliname>prior-less-100ms</cliname>
				<dbTable>portVlanTbl</dbTable>
				<dbAttribute>plist</dbAttribute>
				<index>slot;logic</index>
				<parser>bool:2</parser>
				<novalue>true</novalue>
			</attribute>
			<attribute>
				<cliname>prior-less-10ms</cliname>
				<dbTable>portVlanTbl</dbTable>
				<dbAttribute>plist</dbAttribute>
				<index>slot;logic</index>
				<parser>bool:1</parser>
				<novalue>true</novalue>
			</attribute>
			<attribute>
				<cliname>prior-nw-ctrl</cliname>
				<dbTable>portVlanTbl</dbTable>
				<dbAttribute>plist</dbAttribute>
				<index>slot;logic</index>
				<parser>bool:0</parser>
				<novalue>true</novalue>
			</attribute>
			<attribute>
				<cliname>in-qos-prof-name</cliname>
				<dbTable>portVlanTbl</dbTable>
				<dbAttribute>tcProId</dbAttribute>
				<index>slot;logic</index>
				<parser>ref|name:|(QosInProf:Index:Name)</parser>
				<default>name:Default_TC0</default>
			</attribute>
			<attribute>
				<cliname>max-up-qos-policy</cliname>
				<dbTable>portVlanTbl</dbTable>
				<dbAttribute>polNbr</dbAttribute>
				<index>slot;logic</index>
				<default>0</default>
			</attribute>
			<attribute>
				<cliname>max-ip-antispoof</cliname>
				<dbTable>portVlanTbl</dbTable>
				<dbAttribute>staIp</dbAttribute>
				<index>slot;logic</index>
				<default>65535</default>
			</attribute>
			<attribute>
				<cliname>max-unicast-mac</cliname>
				<dbTable>portVlanTbl</dbTable>
				<dbAttribute>maxMac</dbAttribute>
				<index>slot;logic</index>
				<default>65535</default>
			</attribute>
		</attributes>
	</mapping>
</mappings>
