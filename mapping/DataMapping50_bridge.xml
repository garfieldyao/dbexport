<?xml version="1.0" encoding="UTF-8"?>
<mappings xmlns="http://www.garfield.com/MappingSchema"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.garfield.com/MappingSchema MappingSchema.xsd ">
	<!-- configure vlan id -->
	<mapping>
		<attributes>
			<prefix>configure bridge</prefix>
			<miniAttributes>2</miniAttributes>
			<sortor>com.mars.dbexport.service.sortor.GenericSortor</sortor>
			<attribute>
				<cliname>port</cliname>
				<dbTable>portTbl</dbTable>
				<dbAttribute>bsLog</dbAttribute>
				<index>bsSlot;bsLog</index>
				<parser>multi(xref|1/1/|lanConfig|(bsSlot,2,4:index,2,4)(bsLog,2,4:itfmLog,2,4)|(index,16,8,-2)(raw:/)(index,7,9,1)(raw:/)(index,0,7,1)(raw:/)(slot,0,0,0)(raw:/)(port,0,0,0);xref|1/1/|voipUni|(bsSlot,2,4:index,2,4)(bsLog,2,4:itfmLog,2,4)|(index,16,8,-2)(raw:/)(index,7,9,1)(raw:/)(index,0,7,1)(raw:/)(raw:vuni))</parser>
			</attribute>
			<attribute>
				<cliname>pvid</cliname>
				<dbTable>portTbl</dbTable>
				<dbAttribute>pvid</dbAttribute>
				<index>bsSlot;bsLog</index>
				<parser>class:com.mars.dbexport.service.parse.VlanIdParser</parser>
			</attribute>
			<attribute>
				<cliname>default-priority</cliname>
				<dbTable>portTbl</dbTable>
				<dbAttribute>portPr</dbAttribute>
				<index>bsSlot;bsLog</index>
				<default>0</default>
			</attribute>
			<attribute>
				<cliname>mac-learn-off</cliname>
				<dbTable>portTbl</dbTable>
				<dbAttribute>lnStat</dbAttribute>
				<index>bsSlot;bsLog</index>
				<parser>bool:-0</parser>
				<novalue>true</novalue>
			</attribute>
			<attribute>
				<cliname>max-unicast-mac</cliname>
				<dbTable>portTbl</dbTable>
				<dbAttribute>maxAdd</dbAttribute>
				<index>bsSlot;bsLog</index>
				<default>1</default>
			</attribute>
			<attribute>
				<cliname>qos-profile</cliname>
				<dbTable>portTbl</dbTable>
				<dbAttribute>qosProf</dbAttribute>
				<index>bsSlot;bsLog</index>
				<parser>ref|name:|(QosSessions:Index:Name)</parser>
			</attribute>
			<attribute>
				<cliname>prio-regen-prof</cliname>
				<dbTable>portTbl</dbTable>
				<dbAttribute>priReg</dbAttribute>
				<index>bsSlot;bsLog</index>
				<parser>ref|(regtbl:id:name)</parser>
			</attribute>
			<attribute>
				<cliname>max-committed-mac</cliname>
				<dbTable>portTbl</dbTable>
				<dbAttribute>cmtMax</dbAttribute>
				<index>bsSlot;bsLog</index>
				<default>65535</default>
			</attribute>
			<attribute>
				<cliname>mirror-mode</cliname>
				<dbTable>portTbl</dbTable>
				<dbAttribute>mirMode</dbAttribute>
				<index>bsSlot;bsLog</index>
				<parser>enum:(0x0,disable)(0x3,overwrite-outer-vlan)</parser>
				<default>disable</default>
			</attribute>
			<attribute>
				<cliname>mirror-vlan</cliname>
				<dbTable>portTbl</dbTable>
				<dbAttribute>mirVlan</dbAttribute>
				<index>bsSlot;bsLog</index>
				<default>0</default>
			</attribute>
			<attribute>
				<cliname>pvid-tagging-flag</cliname>
				<dbTable>portTbl</dbTable>
				<dbAttribute>pvidTag</dbAttribute>
				<index>bsSlot;bsLog</index>
				<parser>enum:(0x0,onu)(0x1,olt)</parser>
				<default>onu</default>
			</attribute>
		</attributes>
	</mapping>
</mappings>
